  inherited qyExplorar: TIBQuery
    SQL.Strings = (
      'SELECT SC.ID R_ID,'
      '       S.FECHA_ALTA R_FECHA,'
      '       S.CLAVE R_C_CLAVE,'
      '       S.NOMBRE R_CLIENTE,'
      '       S.RAZON_SOCIAL R_RAZON_SOCIAL,'
      '       P.CODIGO_TEL R_CODIGO_PAIS,'
      '       E.CODIGO_TEL R_CODIGO_ESTADO,'
      '       S.TEL_PRINCIPAL R_TELEFONO,'
      '       S.RFC R_RFC,'
      '       S.CURP R_CURP,'
      '       SC.LIMITE_CREDITO R_LIMITE,'
      '       D.CALLE R_CALLE,'
      '       D.ENTRE_CALLE R_ENTRE,'
      '       D.Y_CALLE R_Y,'
      '       D.NUM_EXT R_NUM_EXT,'
      '       D.LETRA R_LETRA,'
      '       D.NUM_INT R_NUM_INT,'
      '       D.COLONIA R_COLONIA,'
      '       D.CP R_CP,'
      '       C.DESCRIPCION R_CIUDAD,'
      '       E.DESCRIPCION R_ESTADO,'
      '       P.DESCRIPCION R_PAIS,'
      '       S2.CLAVE R_VENDEDOR_C,'
      '       S2.NOMBRE R_VENDEDOR, '
      '       SC.ESTATUS R_ESTATUS,'
      '       SC.FECHA_ULT_COMPRA R_FECHA_ULT_COMPRA,'
      '       SC.FECHA_ULT_PAGO R_FECHA_ULT_PAGO,'
      '       S.E_MAIL R_CORREO,'
      '       SC.DP_PORC R_DP_PORC,'
      '       SC.DESCUENTO_PORC  R_DG_PORC,'
      '       S.SEP_NOMBRE R_SEP_NOMBRE,'
      '       S.SEP_APATERNO R_SEP_APATERNO,'
      '       S.SEP_AMATERNO R_SEP_AMATERNO,'
      '       SC.DIAS_DE_RETARDO R_DIAS_DE_RETARDO,'
      '       CC.CLAVE R_CC_C,'
      '       CC.NOMBRE R_CC_N,'
      '       CL.DESCRIPCION R_CLASIFICACION_D'
      
        'FROM (SUJETOS_CLIENTES SC LEFT JOIN SUJETOS S ON SC.SUJETO = S.I' +
        'D)'
      '     LEFT JOIN DOMICILIOS D ON S.DOMICILIO = D.ID'
      '     LEFT JOIN ENTIDAD_GEOGRAFICA C ON D.CIUDAD = C.ID'
      '     LEFT JOIN ENTIDAD_GEOGRAFICA E ON C.LINK   = E.ID'
      '     LEFT JOIN ENTIDAD_GEOGRAFICA P ON E.LINK   = P.ID'
      '     LEFT JOIN SUJETOS_VENDEDORES SV ON SC.VENDEDOR = SV.ID'
      '     LEFT JOIN SUJETOS S2 ON SV.SUJETO = S2.ID'
      
        '     LEFT JOIN CONDICIONES_COMERCIALES CC ON SC.FORMAS_DE_PAGO =' +
        ' CC.ID'
      '     LEFT JOIN CLASIFICACIONES CL ON SC.CLASIFICACION = CL.ID'
      'WHERE SC.ID = 0')
    Left = 101
