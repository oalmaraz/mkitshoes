  inherited qyExplorar: TIBQuery
    OnCalcFields = qyExplorarCalcFields
    Active = True
    SQL.Strings = (
      'SELECT F.ID R_ID, '
      '       F.CLAVE R_CLAVE, '
      '       F.FECHA_ALTA R_FECHA_ALTA, '
      '       S.CLAVE R_C_CLIENTE,'
      '       F.NOMBRE R_CLIENTE, '
      '       F.TELEFONO R_TELEFONO, '
      '       F.CIUDAD R_CIUDAD, '
      '       CE.CLAVE R_E_CLAVE,'
      '       CE.NOMBRE R_EXTENSION,'
      '       F.CONSECUTIVO R_FACTURA, '
      '       F.GRANTOTAL R_GRANTOTAL, '
      '       F.ESTATUS R_ESTATUS, '
      '       A.CLAVE R_ALMACEN, '
      '       S2.CLAVE R_V_CLAVE,'
      '       S2.NOMBRE R_VENDEDOR,'
      '       CC.CLAVE R_CC_CLAVE, CC.NOMBRE R_CC_NOMBRE,'
      '       F.SEGURO_SUBTOTAL R_SEGURO_SUBTOTAL,'
      '       F.SEGURO_IVA R_SEGURO_IVA,'
      '       Z.DESCRIPCION R_ZONA_D,'
      '       SUM(FD.CANTIDAD) R_CANTIDAD'
      
        'FROM (FACTURAS F LEFT JOIN SUJETOS_CLIENTES SC ON F.CLIENTE = SC' +
        '.ID)'
      '     LEFT JOIN SUJETOS S ON SC.SUJETO = S.ID'
      '     LEFT JOIN DOMICILIOS D ON S.DOMICILIO = D.ID'
      '     LEFT JOIN CLIENTES_EXTENSIONES CE ON F.EXTENSION = CE.ID'
      '     LEFT JOIN SUJETOS_VENDEDORES SV ON SC.VENDEDOR = SV.ID'
      '     LEFT JOIN SUJETOS S2 ON SV.SUJETO = S2.ID'
      '     LEFT JOIN ALMACENES A ON F.ALMACEN = A.ID'
      
        '     LEFT JOIN CONDICIONES_COMERCIALES CC ON F.CONDICION_COMERCI' +
        'AL = CC.ID'
      '     LEFT JOIN ZONAS Z ON D.ZONA_ = Z.ID'
      '     LEFT JOIN FACTURAS_DETALLE FD ON F.ID = FD.FACTURA'
      'WHERE F.ID = 0'
      'GROUP BY F.ID, '
      '         F.CLAVE, '
      '         F.FECHA_ALTA, '
      '         S.CLAVE,'
      '         F.NOMBRE, '
      '         F.TELEFONO, '
      '         F.CIUDAD, '
      '         CE.CLAVE,'
      '         CE.NOMBRE,'
      '         F.CONSECUTIVO, '
      '         F.GRANTOTAL, '
      '         F.ESTATUS, '
      '         A.CLAVE, '
      '         S2.CLAVE,'
      '         S2.NOMBRE,'
      '         CC.CLAVE, CC.NOMBRE,'
      '         F.SEGURO_SUBTOTAL,'
      '         F.SEGURO_IVA,'
      '         Z.DESCRIPCION')
  end
